{
  "name": "diogenes-lantern",
  "version": "0.0.1",
  "description": "It helps debugging diogenes",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build/*",
    "test": "mocha tests/*.js",
    "watch": "npm run test -- -w",
    "lint": "eslint --fix --ext .js ./src ./tests ./demo",
    "watch-demo-js": "watchify demo/demo.js -o build/demo.js -v",
    "watch-demo-html": "catw demo/index.html -o build/index.html -v",
    "watch-demo-css": "catw node_modules/normalize.css/normalize.css src/lantern.css -o build/lantern.css -v",
    "release:major": "npm-release major",
    "release:minor": "npm-release minor",
    "release:patch": "npm-release patch",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "serve": "http-server build",
    "dev": "npm run watch-demo-js & npm run watch-demo-css & npm run watch-demo-html & npm run serve"
  },
  "keywords": [
    "plugins",
    "dependency injection",
    "services"
  ],
  "repository": "git@github.com:sithmel/diogenes-lantern.git",
  "author": "Maurizio Lupo <maurizio.lupo@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^16.1.1",
    "catw": "^1.0.1",
    "chai": "^4.1.2",
    "diogenes": "^3.0.1",
    "eslint": "^4.18.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "mocha": "^5.0.1",
    "normalize.css": "^8.0.0",
    "npm-release": "^1.0.0",
    "rimraf": "^2.5.2",
    "watchify": "^3.11.0"
  },
  "dependencies": {
    "d3": "^4.8.0"
  }
}
