{
  "name": "diogenes-lantern",
  "version": "0.0.1",
  "description": "It helps debugging diogenes",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build/*",
    "test": "mocha tests/*.js",
    "watch": "npm run test -- -w",
    "lint": "./node_modules/.bin/eslint --ext .js ./src ./tests",
    "watch-demo-js": "./node_modules/.bin/watchify demo/demo.js -o build/demo.js -v",
    "watch-demo-html": "./node_modules/.bin/catw demo/index.html -o build/index.html -v",
    "watch-demo-css": "./node_modules/.bin/catw src/lantern.css -o build/lantern.css -v",
    "release:major": "./node_modules/.bin/npm-release major",
    "release:minor": "./node_modules/.bin/npm-release minor",
    "release:patch": "./node_modules/.bin/npm-release patch",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "serve": "./node_modules/.bin/http-server build",
    "dev": "npm run watch-demo-js & npm run watch-demo-css & npm run watch-demo-html & npm run serve"
  },
  "keywords": [
    "plugins",
    "dependency injection",
    "services"
  ],
  "repository": "git@github.com:sithmel/diogenes-lamp.git",
  "author": "Maurizio Lupo <maurizio.lupo@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^13.0.0",
    "catw": "^1.0.1",
    "chai": "^1.10.0",
    "eslint": "^1.10.3",
    "http-server": "^0.8.5",
    "husky": "^0.10.2",
    "mocha": "^2.1.0",
    "npm-release": "^1.0.0",
    "rimraf": "^2.5.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "async-deco": "^4.3.0",
    "d3": "^3.5.16",
    "diogenes": "^1.0.0",
    "occamsrazor": "^5.0.0"
  }
}
