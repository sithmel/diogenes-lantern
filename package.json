{
  "name": "diogenes-lantern",
  "version": "0.0.1",
  "description": "It helps debugging diogenes",
  "main": "index.js",
  "scripts": {
    "test": "mocha tests/*.js",
    "watch": "npm run test -- -w",
    "lint": "./node_modules/.bin/eslint --ext .js ./src ./tests",
    "build-demo": "./node_modules/.bin/browserify demo/demo.js -o build/demo.js",
    "release:major": "./node_modules/.bin/npm-release major",
    "release:minor": "./node_modules/.bin/npm-release minor",
    "release:patch": "./node_modules/.bin/npm-release patch",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "serve": "http-server"
  },
  "keywords": [
    "plugins",
    "dependency injection",
    "services"
  ],
  "repository": "git@github.com:sithmel/diogenes-lamp.git",
  "author": "Maurizio Lupo <maurizio.lupo@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^13.0.0",
    "chai": "^1.10.0",
    "eslint": "^1.10.3",
    "http-server": "^0.8.5",
    "husky": "^0.10.2",
    "mocha": "^2.1.0",
    "npm-release": "^1.0.0"
  },
  "dependencies": {
    "async-deco": "^4.2.1",
    "d3": "^3.5.16",
    "diogenes": "^0.6.0",
    "memoize-cache": "^1.0.2",
    "occamsrazor": "^5.0.0",
    "occamsrazor-validator": "^5.1.0",
    "setimmediate": "^1.0.4"
  }
}
